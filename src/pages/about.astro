---
import PageLayout from "@/layouts/PageLayout.astro";
import AboutSection from "@/components/sections/About.astro";
import Container from "@/components/primitives/Container.astro"
import { getAboutData } from "../lib/appwrite/client";
import pagesData from "@/data/global_pages.json";

type AboutPageData = {
  content?: {
    title?: string;
    subtitle?: string;
    content?: string;
  };
};

const data = pagesData?.about;

try {
  await getAboutData()
} catch (error) {
  console.log(error);
  
}

---
<PageLayout data={data}>  
  <Container>
      <AboutSection 
      title={data?.content?.title} 
      subtitle={data?.content?.subtitle} 
      content={data?.content?.content} 
      />
    </Container>
</PageLayout>